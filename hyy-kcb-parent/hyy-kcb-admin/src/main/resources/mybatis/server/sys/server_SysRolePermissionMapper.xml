<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hyy.kcb.dao.sys.ISysRolePermissionDao" >

    <select id="findRolePermissionById" parameterType="java.lang.Long" resultMap="sysRolePermissionResultMap">
        select * from sys_role_permission where sys_role_id =#{roleId} and sys_permission_id = #{permissionId}
    </select>
</mapper>